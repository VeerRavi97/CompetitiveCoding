
#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
typedef vector <ll > vi;
#define all(c) (c).begin(),(c).end()

int main()
{

   // ios::sync_with_stdio(0);
   // cin.tie(0);
    ll t;
    cin >> t;

    while(t--)
    {

        ll n;
        cin >> n;
        vi v(n);
        vi arr(8);
        arr[0]=1;
        vi l;vi r;

        for(int i=0;i<n;i++)
        {

            int x;
            cin >> x;
            v[i]=x;
        }
        if(n<13 )
             cout << "no" << "\n";

        else
        {

           int first=find(all(v),7)-v.begin();
           if(find(all(v),7)!=v.end())
           {
             int last=(find(v.rbegin(),v.rend(),7)-v.rbegin());
            last=n-last-1;


        //   cout << "first " << first << "last " << last << "\n";


             for(ll i=0;i<=first;i++)
                l.push_back(v[i]);

           for(ll i=n-1;i>=last;i--){
                r.push_back(v[i]);
            }

   //  cout << "\n";
         //   for(int i=0;i<=first;i++)
         //     cout << l[i] << " ";
         //   cout << "\n";
        //   for(int i=0;i<=last;i++)
       //        cout << r[i] << " ";
       //   cout << "\n";
            int sz1=l.size();
            int sz2=r.size();
        //    cout << "sizes " << sz1 << " " << sz2;
         //    cout << "\n";
            if(sz1==sz2)
            {

            for(int i=0;i<=sz1;i++)
            {
                if(l[i]==r[i] && l[i]<=7)
                {
                    arr[l[i]]=1;
                }


            }
            //for(int i=0;i<arr.size();i++)
           //   cout  << arr[i] << " ";
            //   cout << "\n";
            int coun=count(arr.begin(),arr.end(),1);
            if(coun==8)
                cout << "yes" << "\n";
            else
                cout << "no" << "\n";
            }
            else
                cout << "no" << "\n";
           }
           else
                cout << "no" << "\n";
           }


        }

    }








