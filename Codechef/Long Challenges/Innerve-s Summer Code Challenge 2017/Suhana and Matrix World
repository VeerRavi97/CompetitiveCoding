
#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
int main()
{
    ios:: sync_with_stdio(0);
    cin.tie(0);
    ll tc;
    cin >> tc;
    while(tc--)
    {
        ll m,x=0,y=0;
        cin >> m;
        if(m%3==1 || m%3==2)
        {
            x=(m/3)+1;
            y=m-x;
        }
        else
        {
            x=m/3;
            y=m-x;
        }


       cout << x << " " << y << "\n";

    }














}
