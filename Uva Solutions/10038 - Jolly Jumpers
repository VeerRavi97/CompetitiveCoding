
#include<bits/stdc++.h>
using namespace std;
typedef vector <int> vi;


int main()
{

    int n;

     int x;


vi :: iterator it;


    while(cin >> n)
    {
         vi v1;
         int count=0;


vi v2(n,0);


        for(int i=0;i<n;i++)
        {

            scanf("%d",&x);
            v1.push_back(x);


        }


        for(int i=0;i<n-1;i++)
        {
            int k;
            k=abs(v1[i]-v1[i+1]);
            if(k>0 && k < n)
            v2[k-1]=1;



        }
for(int i=0;i<n;i++)
{

    if(v2[i]>0)
        count+=1;


}


if(count ==n-1)

printf("Jolly\n");
else
    printf("Not jolly\n");
}
return 0;
}
