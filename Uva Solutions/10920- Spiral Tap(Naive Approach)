#include<bits/stdc++.h>
using namespace std;

int main()
{
    int dx[4]={1,0,-1,0};
    int dy[4]={0,-1,0,1};
    int N,n;
     int count=0;
     int x,y;
     int ch;
    while((scanf("%d%d",&N,&n)),(N|| n))
    {

       int total=1;
        x=(N+1)/2;
        y=(N+1)/2;
        count =1;
ch=0;
        bool done=false;
       while(total < n)
        {

            for(int i=1;i<=2*count;i++)
            {

                if(i==count+1)
                    ch++;
                  if(ch==4) ch=0;
                x+=dx[ch];
                y+=dy[ch];
                total++;
            if(total==n)
                {
                    done=true;
                    break;
                }

            }
            if(done) break;


            if(ch==4) ch=0; else ch++;
            count++;
        }


        printf("LINE = %d, COLUMN = %d\n",x,y);
    }
}


