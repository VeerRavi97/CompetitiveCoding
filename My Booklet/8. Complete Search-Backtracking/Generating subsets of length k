// use __ builtin_popcount(x)==k 
#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
typedef vector <ll> vi;
#define all(c) c.begin(),c.end()
int main()
{

    ll n;

    cin >> n;
vi v(n,0);
    for( ll i=0;i<n ;i++)
    {
       ll in;
       cin >> in;
       v[i]=in;

    }

    for( ll i=0;i<(1<<n) ;i++)
    {
if    (__builtin_popcount(i)==2)
      {


       vi subset;

        for(ll k=0;k<n;k++)
        {


        if (i&(1<<k))
        {
            cout << v[k] << " ";

         subset.push_back(v[k]);
        }
        }

cout << " \n";
      }
    }




}
