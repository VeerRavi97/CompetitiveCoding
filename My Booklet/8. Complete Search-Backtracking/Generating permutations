#include<bits/stdc++.h>
using namespace std;
char ch[51];
int main()
{

    int n[3]={3,2,1};
    sort(n,n+3);


    do
    {
        for(int i=0;i<3;i++)
        printf("%d",n[i]);
        printf("\n");

    }while(next_permutation(n,n+3)); // returns true if the function could rearrange the object
    // as a lexicographically greater permutation,otherwise returns false
