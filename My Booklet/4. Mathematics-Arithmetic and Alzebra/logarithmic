
#include<bits/stdc++.h>
using namespace std;
int main()
{

    cout << log(20)/log(3)<< "\n"; // log3(20)=loge(20)/loge(3)
    cout << log(20) << " "; // loge(20)
    cout << log(3) << " "; //loge(3)
    cout << log2(10) << " ";//valid for base 2 and 10
    cout << log10(10) << " ";




}

